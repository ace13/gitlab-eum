{
  "name": "gitlab-eum",
  "version": "0.0.0",
  "description": "GitLab External User Manager",
  "author": "Alexander Olofsson <alexander.olofsson@liu.se>",
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e client/main.js -o public/js/build.js",
    "serve": "node index.js",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -g envify client/main.js | uglifyjs -c warnings=false -m > public/js/build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.liu.se/aleol57/gitlab-eum.git"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "gitlab": "^1.7.1",
    "jquery": "^3.2.1",
    "passport": "^0.4.0",
    "vue": "^2.0.1",
    "vue-model": "^2.0.7"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.6",
    "envify": "^3.4.1",
    "http-server": "^0.9.0",
    "npm-run-all": "^2.1.2",
    "uglify-js": "^2.5.0",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "browser": {
    "boostrap": "bootstrap/dist/js/bootstrap.min.js",
    "jquery": "jquery/dist/jquery.min.js",
    "vue": "vue/dist/vue.common.js"
  }
}
